{{define "list"}}
<ul>
    {{range .Items}}
        {{if .IsURL}}
        <li><a href="{{.URL}}">{{.Value}}</a></li>
        {{end}}

        {{if .IsText}}
        <li>{{.Value}}</li>
        {{end}}

        {{if .IsBlock}}
        <li><blockquote>{{.Value}}</blockquote></li>
        {{end}}

        {{if .IsHeaderOne}}
        <li><h2 class="text-xl font-bold">{{.Value}}</h2></li>
        {{end}}

        {{if .IsHeaderTwo}}
        <li><h3 class="text-lg font-bold">{{.Value}}</h3></li>
        {{end}}
    {{end}}
</ul>
{{end}}
